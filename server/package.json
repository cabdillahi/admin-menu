    "express": "^5.1.0",
    "init": "^0.1.2",
    "jio": "^3.45.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "next-themes": "^0.4.6",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "redis": "^5.8.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
